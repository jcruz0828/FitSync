{
  "expo": {
    "name": "FitSync",
    "slug": "FitSync",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "FitSync",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra":{
      "APPWRITE_ENDPOINT" :"https://cloud.appwrite.io/v1",
      "APPWRITE_PLATFORM" : "com.jcoytfined.fitsync",
      "APPWRITE_PROJECT" : "674e546f000853f8d623",
      "APPWRITE_USER" : "674e550100130ce43ad1",
      "APPWRITE_DB" : "674e54fb000ef74af0d9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
